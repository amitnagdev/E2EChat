{
    "favIcon": "./public/modules/core/img/brand/favicon/favicon.png",
    "modelFiles": "./app/data/models/**/*.js",
    "routeFiles": "./app/facade/**/routes/**/*.js",
    "staticDataFiles": "./app/static/*.js",
    "microservices": "./app/microservices/**/*.*service.js",
    "cronJobs": "./app/jobs/**/*.job.js",
    "socketIOFiles": "./app/socket.io/**/*.*socket.io.js",
    "userDto": "./app/dto/user.token.server.dto",
    "blacklistFile": null,
    "spaHtml": "./app/views/index.html",
    "html5RoutingPresenter": "./app/facade/ui/presenters/core.server.presenter",
    "viewEngine": "swig",
    "viewsExt": "server.view.html",
    "viewsDir": "./app/facade/ui/views",
    "staticDir": "./public",
    "poweredBy": "e2e-framework",
    "customMiddleware": "./app/middlewares",
    "httpMiddleware": "./app/middlewares/http",
    "httpStaticDir": "./public",
    "config": {
        "cryptoAlgorithm": "aes-256-ctr",
        "baseDir": "./config/env/",
        "genericFile": "all",
        "encryptFile": ".env.kvp",
        "decryptFile": ".env.plain.js",
        "passwordFile": ".pwd"
    },
    "ssl": {
        "baseDir": "./config/sslcerts/",
        "mongo": {
            "baseDir": "./config/sslcerts/mongo/",
            "cert": "mongodb.pem",
            "ca": "ca.pem"
        }
    },
    "ga": {
        "url": "/e2efa",
        "routesWhiteListed": [ "/logs", "/sys/info", "/modules/", "/dist/", "/lib/", "/img/", "/i18n/", "/images/", "/assets/", "/js/" ]
    }
}